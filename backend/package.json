{
  "name": "siber-zed-backend",
  "version": "1.0.0",
  "description": "Backend API for SiberZed Security Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=5001 nodemon src/index.ts",
    "dev:local": "cross-env NODE_ENV=development PORT=5001 nodemon --env-file=.env.development src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "npm run db:generate && node dist/index.js",
    "start:internal": "cross-env NODE_ENV=production node dist/index.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo Build completed successfully",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229 src/index.ts",
    "check-deps": "npm outdated",
    "setup": "npm install && npm run build && npm run db:generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.2",
    "chrome-launcher": "^1.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lighthouse": "^13.0.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.1",
    "puppeteer": "^24.33.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "wappalyzer": "^7.0.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.22",
    "@types/nodemailer": "^6.4.19",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}